<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PracticeComposeBasics"
        tools:targetApi="31">
        <activity
            android:name=".BusinessCardActivity"
            android:exported="false"
            android:label="@string/title_activity_business_card"
            android:theme="@style/Theme.PracticeComposeBasics" />
        <activity
            android:name=".ComposeQuadrantActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_quadrant"
            android:theme="@style/Theme.PracticeComposeBasics" />
        <activity
            android:name=".TaskManagerActivity"
            android:exported="false"
            android:label="@string/title_activity_task_manager"
            android:theme="@style/Theme.PracticeComposeBasics" />
        <activity
            android:name=".ComposeArticle"
            android:exported="false"
            android:label="@string/title_activity_compose_article"
            android:theme="@style/Theme.PracticeComposeBasics" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.PracticeComposeBasics">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>